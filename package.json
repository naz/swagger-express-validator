{
  "name": "swagger-express-validator",
  "version": "1.0.2",
  "engines": {
    "node": ">=6.0.0"
  },
  "description": "Validation middleware that validates request/response against provided swagger spec",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gargol/swagger-express-validator"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha ./test --ui bdd",
    "lint": "eslint ./",
    "lint-fix": "eslint ./ --fix"
  },
  "keywords": [
    "validation",
    "swagger",
    "middleware",
    "connect",
    "express"
  ],
  "author": "Naz",
  "license": "MIT",
  "dependencies": {
    "ajv": "^6.10.2",
    "debug": "3.2.6",
    "lodash": "^4.17.15",
    "path-to-regexp": "2.4.0",
    "validator": "10.11.0"
  },
  "devDependencies": {
    "body-parser": "1.20.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.18.2",
    "express": "4.18.2",
    "http-status-codes": "1.3.2",
    "husky": "1.3.1",
    "middleware-testlab": "2.5.0",
    "mocha": "6.2.0",
    "supertest": "4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "pre-push": "npm run lint && npm test"
    }
  },
  "files": [
    "README.md",
    "LICENSE",
    "index.js"
  ]
}
